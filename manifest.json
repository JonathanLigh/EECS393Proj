{
  "manifest_version": 2,

  "name": "RRE",
  "description": "",
  "version": "1.1",

  "browser_action": {
    "default_icon": "src/img/rre-icon-128.png",
    "default_popup": "src/popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/build/main.js"]
    }
  ]
}
